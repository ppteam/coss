<assembly>
	<id>dev</id>
	<formats>
		<format>zip</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<fileSets>
		<fileSet>
			<directory>run</directory>
			<outputDirectory></outputDirectory>
			<fileMode>0755</fileMode>
		</fileSet>
		<fileSet>
			<directory>template</directory>
			<outputDirectory>template</outputDirectory>
		</fileSet>
	</fileSets>
	<dependencySets>
		<dependencySet>
			<outputDirectory>/lib</outputDirectory>
			<includes>
				<include>com.googlecode.coss:coss.tool.generator:jar</include>
				<include>org.freemarker:freemarker:jar</include>
				<include>com.oracle:ojdbc14:jar</include>
				<include>mysql:mysql-connector-java:jar</include>
			</includes>
			<useTransitiveDependencies>true</useTransitiveDependencies>
			<!-- <outputFileNameMapping>coss.war</outputFileNameMapping> -->
			<unpack>false</unpack>
		</dependencySet>
	</dependencySets>
</assembly>