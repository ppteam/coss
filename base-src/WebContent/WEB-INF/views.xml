		parent="AbstractXlsView">
		<property name="xlsPath" value="classpath:com/neusoft/xlsTemplat/wc_assist.xls" />
	</bean>
	<bean id="CheckDetailTotalViewResolvers" class="com.neusoft.model.reportforms.service.CheckDetailTotalViewResolvers"
		parent="AbstractXlsView">
		<property name="xlsPath" value="classpath:com/neusoft/xlsTemplat/ck_totals.xls" />
	</bean>
</beans>
