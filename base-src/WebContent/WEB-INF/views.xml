	</bean>
	<bean id="StaffInfoViewResolvers" class="com.neusoft.model.reportforms.service.StaffInfoViewResolvers"
		parent="AbstractXlsView">
		<property name="xlsPath" value="classpath:com/neusoft/xlsTemplat/staffBasicInfo.xls" />
	</bean>
	<bean id="AddrBookViewResolvers" class="com.neusoft.model.reportforms.service.AddrBookViewResolvers"
		parent="AbstractXlsView">
		<property name="xlsPath" value="classpath:com/neusoft/xlsTemplat/AddressBook.xls" />
	</bean>
	<bean id="ProjWeekViewResolvers" class="com.neusoft.model.reportforms.service.ProjWeekViewResolvers"
		parent="AbstractXlsView">
		<property name="xlsPath" value="classpath:com/neusoft/xlsTemplat/proweekforms.xls" />
