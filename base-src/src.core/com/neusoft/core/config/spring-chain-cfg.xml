<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd">
	<context:property-placeholder
		location="classpath:com/neusoft/config/system-context-cfg.properties" />
	<!-- chian support config -->
	<bean id="ChainSupport" class="com.neusoft.core.web.ChainSupport"
		abstract="true" />
	<bean id="ChainBase" class="com.neusoft.core.chain.Impl.ChainBase"
		scope="prototype" />
	<bean id="FilterBase" class="com.neusoft.core.chain.Impl.FilterBase"
		abstract="true">
		<property name="transactionManager" ref="transactionManager" />
		<property name="errorHolderUtil" ref="errorHolderUtil" />
	</bean>
	<bean id="SqlMapClientSupportFilter"
		class="com.neusoft.core.chain.general.SqlMapClientSupportFilter"
		parent="FilterBase" abstract="true">
		<property name="sqlMapClientTemplate" ref="sqlMapClientTemplate" />
	</bean>
	<bean id="AbstractQueryFilter" class="com.neusoft.core.chain.general.AbstractQueryFilter"
		parent="SqlMapClientSupportFilter" abstract="true" />
	<bean id="AbstractAjaxAction" class="com.neusoft.core.web.AbstractAjaxAction"
		parent="ChainSupport" abstract="true" />
	<bean id="AbstractController" class="com.neusoft.core.web.AbstractController"
		parent="ChainSupport" abstract="true" />
</beans>
